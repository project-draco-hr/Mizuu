{
  try {
    FileSource fs=getFileSource();
    SmbFile root=new SmbFile(MizLib.createSmbLoginString(URLEncoder.encode(fs.getDomain(),"utf-8"),URLEncoder.encode(fs.getUser(),"utf-8"),URLEncoder.encode(fs.getPassword(),"utf-8"),fs.getFilepath(),true));
    return root;
  }
 catch (  Exception e) {
  }
  return null;
}
