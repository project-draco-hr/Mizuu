{
  DbAdapterTvShowEpisode dbHelper=MizuuApplication.getTvEpisodeDbAdapter();
  Cursor cursor=dbHelper.getAllEpisodesInDatabase(ignoreRemovedFiles());
  ColumnIndexCache cache=new ColumnIndexCache();
  try {
    while (cursor.moveToNext())     existingEpisodes.put(cursor.getString(cache.getColumnIndex(cursor,DbAdapterTvShowEpisode.KEY_FILEPATH)),"");
  }
 catch (  Exception e) {
  }
 finally {
    cursor.close();
    cache.clear();
  }
  TreeSet<String> results=new TreeSet<String>();
  recursiveSearch(getFolder(),results);
  List<String> list=new ArrayList<String>();
  Iterator<String> it=results.iterator();
  while (it.hasNext())   list.add(it.next());
  return list;
}
