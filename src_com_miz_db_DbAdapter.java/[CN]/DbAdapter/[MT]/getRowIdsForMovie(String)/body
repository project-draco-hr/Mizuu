{
  Cursor mCursor=database.query(true,DATABASE_TABLE,SELECT_ALL,KEY_TMDBID + "='" + movieId+ "'",null,null,null,null,null);
  if (mCursor == null)   return null;
  MovieVersion[] results=new MovieVersion[mCursor.getCount()];
  int count=0;
  while (mCursor.moveToNext()) {
    results[count]=new MovieVersion(Integer.valueOf(mCursor.getString(mCursor.getColumnIndex(DbAdapter.KEY_ROWID))),mCursor.getString(mCursor.getColumnIndex(DbAdapter.KEY_FILEPATH)));
    count++;
  }
  mCursor.close();
  return results;
}
