{
  Cursor mCursor=mDatabase.query(true,DATABASE_TABLE,SELECT_ALL,KEY_TMDB_ID + "='" + movieId+ "'",null,null,null,null,null);
  if (mCursor == null)   return false;
  try {
    if (mCursor.getCount() == 0) {
      mCursor.close();
      return false;
    }
  }
 catch (  Exception e) {
    mCursor.close();
    return false;
  }
  mCursor.close();
  return true;
}
