{
switch (type) {
case MovieLoader.ALL_MOVIES:
    return ALL_MOVIES;
case MovieLoader.FAVORITES:
  return FAVORITES;
case MovieLoader.NEW_RELEASES:
return NEW_RELEASES;
case MovieLoader.WATCHLIST:
return WATCHLIST;
case MovieLoader.WATCHED:
return WATCHED;
case MovieLoader.UNWATCHED:
return UNWATCHED;
default :
return COLLECTIONS;
}
}
