{
  try {
    JSONObject jObject=new JSONObject(json);
    JSONArray jArray=jObject.getJSONObject(getArguments().getString("type")).getJSONArray("results");
    pics_sources.clear();
    for (int i=0; i < jArray.length(); i++) {
      if (!MizLib.isAdultContent(getActivity(),jArray.getJSONObject(i).getString("title")) && !MizLib.isAdultContent(getActivity(),jArray.getJSONObject(i).getString("original_title"))) {
        pics_sources.add(new WebMovie(jArray.getJSONObject(i).getString("original_title"),jArray.getJSONObject(i).getString("id"),baseUrl + MizLib.getImageUrlSize(getActivity()) + jArray.getJSONObject(i).getString("poster_path"),MizLib.getPrettyDate(getActivity(),jArray.getJSONObject(i).getString("release_date"))));
      }
    }
    new MoviesInLibraryCheck(pics_sources).execute();
  }
 catch (  Exception e) {
  }
}
