{
  try {
    JSONObject jObject=new JSONObject(mJson);
    JSONArray jArray=jObject.getJSONObject(getArguments().getString("type")).getJSONArray("results");
    mMovies.clear();
    for (int i=0; i < jArray.length(); i++) {
      if (!MizLib.isAdultContent(getActivity(),jArray.getJSONObject(i).getString("title")) && !MizLib.isAdultContent(getActivity(),jArray.getJSONObject(i).getString("original_title"))) {
        mMovies.add(new WebMovie(getActivity().getApplicationContext(),jArray.getJSONObject(i).getString("original_title"),jArray.getJSONObject(i).getString("id"),mBaseUrl + MizLib.getImageUrlSize(getActivity()) + jArray.getJSONObject(i).getString("poster_path"),MizLib.getPrettyDate(getActivity(),jArray.getJSONObject(i).getString("release_date"))));
      }
    }
    new MoviesInLibraryCheck(mMovies).execute();
  }
 catch (  Exception e) {
  }
}
