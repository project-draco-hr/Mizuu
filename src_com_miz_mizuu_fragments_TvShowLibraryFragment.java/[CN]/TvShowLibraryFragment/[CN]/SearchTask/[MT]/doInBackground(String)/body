{
  mTempKeys=new ArrayList<Integer>();
  if (mSearchQuery.startsWith("actor:")) {
    for (int i=0; i < mTvShows.size(); i++) {
      if (isCancelled())       return null;
      if (mTvShows.get(i).getActors().toLowerCase(Locale.ENGLISH).contains(mSearchQuery.replace("actor:","").trim()))       mTempKeys.add(i);
    }
  }
 else {
    String lowerCase="";
    Pattern p=Pattern.compile(MizLib.CHARACTER_REGEX);
    for (int i=0; i < mTvShows.size(); i++) {
      if (isCancelled())       return null;
      lowerCase=mTvShows.get(i).getTitle().toLowerCase(Locale.ENGLISH);
      if (lowerCase.indexOf(mSearchQuery) != -1 || p.matcher(lowerCase).replaceAll("").indexOf(mSearchQuery) != -1)       mTempKeys.add(i);
    }
  }
  return null;
}
