{
  String singleItem="";
  Cursor c=mDatabase.query(DATABASE_TABLE,new String[]{column},KEY_TMDB_ID + " = ?",new String[]{tmdbId},null,null,null);
  if (c != null) {
    try {
      if (c.moveToFirst())       singleItem=c.getString(c.getColumnIndex(column));
    }
 catch (    SQLException e) {
    }
 finally {
      c.close();
    }
  }
  return singleItem;
}
