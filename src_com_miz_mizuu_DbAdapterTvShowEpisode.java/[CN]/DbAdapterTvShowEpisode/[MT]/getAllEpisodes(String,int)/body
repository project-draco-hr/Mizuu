{
  if (type == OLDEST_FIRST)   return database.query(DATABASE_TABLE,allRows,KEY_SHOW_ID + "='" + showId+ "' AND NOT("+ KEY_EPISODE_TITLE+ " = 'MIZ_REMOVED_EPISODE')",null,KEY_SEASON + "," + KEY_EPISODE,null,KEY_SEASON + " asc, " + KEY_EPISODE+ " asc");
 else   return database.query(DATABASE_TABLE,allRows,KEY_SHOW_ID + "='" + showId+ "' AND NOT("+ KEY_EPISODE_TITLE+ " = 'MIZ_REMOVED_EPISODE')",null,KEY_SEASON + "," + KEY_EPISODE,null,KEY_SEASON + " desc, " + KEY_EPISODE+ " desc");
}
