{
  ContentValues updateValues=createContentValues(filepath,coverpath,title,plot,tmdbid,imdbid,rating,tagline,release,certification,runtime,trailer,genres,favourite,watched,collection,collectionId,toWatch,hasWatched,date);
  return database.update(DATABASE_TABLE,updateValues,KEY_ROWID + "=" + rowId,null) > 0;
}
