{
  super.onCreate(b);
  setupDoneDiscard();
  setContentView(R.layout.contact_developer);
  subject=(Spinner)findViewById(R.id.subjectSpinner);
  message=(EditText)findViewById(R.id.traktUsername);
  deviceDetails=(TextView)findViewById(R.id.textView2);
  try {
    String versionType="";
    if (getPackageName().equals("com.miz.mizuulite")) {
      versionType=" (lite)";
    }
    PackageManager manager=getPackageManager();
    PackageInfo packageInfo=manager.getPackageInfo(getPackageName(),0);
    deviceDetails.setText(getString(R.string.contactDeviceModel) + ": " + Build.MODEL+ " ("+ Build.PRODUCT+ ")\n"+ getString(R.string.contactDeviceManufacturer)+ ": "+ Build.MANUFACTURER+ "\n"+ getString(R.string.contactDeviceCustomization)+ ": "+ Build.BRAND+ "\n"+ getString(R.string.contactDeviceOS)+ ": v"+ Build.VERSION.RELEASE+ " ("+ Build.VERSION.INCREMENTAL+ ")\n"+ "Mizuu version: "+ packageInfo.versionName+ versionType);
  }
 catch (  Exception e) {
    Toast.makeText(this,getString(R.string.errorSomethingWentWrong),Toast.LENGTH_SHORT).show();
  }
}
