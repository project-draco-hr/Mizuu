{
  super.onCreate(savedInstanceState);
  if (getLayoutResource() > 0) {
    setContentView(getLayoutResource());
    mToolbar=(Toolbar)findViewById(R.id.toolbar);
    if (mToolbar != null) {
      try {
        setSupportActionBar(mToolbar);
      }
 catch (      Throwable t) {
      }
    }
  }
}
