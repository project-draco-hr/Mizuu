{
  movies.clear();
  DbAdapterMovies dbHelper=MizuuApplication.getMovieAdapter();
  Cursor cursor=dbHelper.fetchAllMovies(DbAdapterMovies.KEY_TITLE + " ASC");
  ColumnIndexCache cache=new ColumnIndexCache();
  try {
    while (cursor.moveToNext()) {
      movies.add(new SmallMovie(mContext,cursor.getString(cache.getColumnIndex(cursor,DbAdapterMovies.KEY_TITLE)),cursor.getString(cache.getColumnIndex(cursor,DbAdapterMovies.KEY_TMDB_ID)),ignorePrefixes));
    }
  }
 catch (  NullPointerException e) {
  }
 finally {
    cursor.close();
    cache.clear();
  }
  Collections.sort(movies);
}
