{
  if (!MizLib.isEmpty(name)) {
    String[] split;
    String year="";
    if (name.contains(" "))     split=name.split(" ");
 else     split=name.split("\\+");
    for (int i=split.length - 1; i > 0; i--) {
      if (split[i].matches("(19|20)[0-9][0-9]")) {
        year=split[i];
        continue;
      }
    }
    name=name.replace(year,"").trim();
  }
  this.name=name;
}
