{
switch (item.getItemId()) {
case R.id.play_video:
    play();
  break;
case R.id.menuDeleteEpisode:
deleteEpisode();
break;
case R.id.watched:
watched(true);
break;
case R.id.identify:
Toast.makeText(getActivity(),"Not yet!",Toast.LENGTH_LONG).show();
break;
case R.id.watchOffline:
watchOffline();
break;
}
return false;
}
