{
  boolean result=mDatabase.delete(DATABASE_TABLE,KEY_TMDB_ID + " = ?",new String[]{tmdbId}) > 0;
  String collectionId=getCollectionId(tmdbId);
  if (!hasMultipleCollectionMappings(collectionId))   result=result && MizuuApplication.getCollectionsAdapter().deleteCollection(collectionId);
  return result && MizuuApplication.getMovieMappingAdapter().deleteMovie(tmdbId);
}
