{
switch (item.getItemId()) {
case R.id.play_video:
    play();
  break;
case R.id.menuDeleteEpisode:
deleteEpisode();
break;
case R.id.watched:
watched(true);
break;
case R.id.identify:
identifyEpisode();
break;
case R.id.watchOffline:
watchOffline();
break;
case R.id.editTvShowEpisode:
editEpisode();
break;
}
return false;
}
