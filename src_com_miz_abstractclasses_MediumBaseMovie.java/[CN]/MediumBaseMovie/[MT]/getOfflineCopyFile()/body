{
  if (mFilepath.contains("<MiZ>"))   return new File(MizLib.getAvailableOfflineFolder(mContext),MizLib.md5(getFilepath()) + "." + MizLib.getFileExtension(getFilepath()));
  return new File(MizLib.getAvailableOfflineFolder(mContext),MizLib.md5(getFullFilepath()) + "." + MizLib.getFileExtension(getFullFilepath()));
}
