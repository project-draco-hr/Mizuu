{
  Cursor cursor=mDatabase.query(DATABASE_TABLE,ALL_ROWS,KEY_TMDB_ID + "='" + tmdbId+ "'",null,null,null,null);
  boolean result=false;
  if (cursor != null) {
    try {
      if (cursor.getCount() > 0)       result=true;
    }
 catch (    Exception e) {
    }
 finally {
      cursor.close();
    }
  }
  return result;
}
