{
  Cursor cursor=mDatabase.query(DATABASE_TABLE,ALL_ROWS,KEY_TMDB_ID + "='" + tmdbId+ "'",null,null,null,null);
  MovieFilepath[] paths=new MovieFilepath[]{};
  if (cursor != null) {
    try {
      paths=new MovieFilepath[cursor.getCount()];
      int counter=0;
      while (cursor.moveToNext()) {
        paths[counter]=new MovieFilepath(cursor.getInt(cursor.getColumnIndex(DbAdapterMovieMapping.KEY_ROWID)),cursor.getString(cursor.getColumnIndex(DbAdapterMovieMapping.KEY_FILEPATH)),cursor.getInt(cursor.getColumnIndex(DbAdapterMovieMapping.KEY_IGNORED)) == 1);
        counter++;
      }
    }
 catch (    Exception e) {
    }
 finally {
      cursor.close();
    }
  }
  return paths;
}
